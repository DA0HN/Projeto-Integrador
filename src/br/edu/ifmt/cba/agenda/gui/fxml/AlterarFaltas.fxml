<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="260.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.edu.ifmt.cba.agenda.gui.controller.AlterarFaltasController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="3.0" layoutY="36.0" prefHeight="27.0" prefWidth="294.0" text="Insira as faltas para alteração">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="btAdicionar" layoutX="76.0" layoutY="184.0" mnemonicParsing="false" onKeyPressed="#btAdicionarOnkeyPressed" onMouseClicked="#btAdicionarOnClicked" text="adicionar" />
      <Button fx:id="btRetirar" layoutX="177.0" layoutY="184.0" mnemonicParsing="false" onKeyPressed="#btRetirarOnKeyPressed" onMouseClicked="#btRetirarOnClicked" text="retirar" />
      <TextField fx:id="txNumeroDeFaltas" alignment="CENTER" layoutX="76.0" layoutY="118.0" promptText="número de faltas" />
   </children>
</AnchorPane>
